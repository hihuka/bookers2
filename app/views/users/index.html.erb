<h2>あいうえお</h2>
<%= form_with model: @user,ul:'users/:id' ,method: :post do |f| %>

  <h4>Title</h4>
  <%= f.text_field :title %>

  <h4>Opinion</h4>
  <%= f.text_area :body %>

  <%= f.submit 'Create Book' %>

<% end %>

<h2>Books</h2>
<table>
   <thead>
      <tr>
        <th>Title</th>
        <th>opinion</th>
        <th colspan="3"></th>
      </tr>
   </thead>
<% @books.each do |book| %>
   <tbody>
      <tr>
        <td><%=book.title%></td>
        <td><%=book.body%></td>
      </tr>
   </tbody>
   <% end %>
</table>